<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="328.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.kuranov.client.ui.ClientStart">
   <VBox alignment="TOP_CENTER" prefHeight="328.0" prefWidth="350.0">
      <children>
         <Label text="Cloud Storage">
            <VBox.margin>
               <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
            </VBox.margin>
            <font>
               <Font size="20.0" />
            </font>
         </Label>
      
                <Label fx:id="label" text="enter login and rassword">
            <VBox.margin>
               <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
            </VBox.margin>
         </Label>
          <TextField fx:id="user" alignment="TOP_CENTER">
            <padding>
               <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </padding>
            <VBox.margin>
               <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
            </VBox.margin>
         </TextField>
          <PasswordField fx:id="password" alignment="TOP_CENTER" onAction="#send">
            <padding>
               <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </padding>
            <VBox.margin>
               <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
            </VBox.margin>
         </PasswordField>
          <Button fx:id="sendAuthButton" mnemonicParsing="false" onAction="#send" text="enter">
            <VBox.margin>
               <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
            </VBox.margin>
         </Button>
      </children>
   </VBox>

</AnchorPane>
